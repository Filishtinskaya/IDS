#!/home/tatjana/Documents/thesis/kitsuneIDS/Kitsune-py/thesis/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'IDS','console_scripts','IDS'
__requires__ = 'IDS'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('IDS', 'console_scripts', 'IDS')()
    )
